						"source(output(",
						"          Dept_ID as string,",
						"          Analyst as string,",
						"          Clerk as string,",
						"          Manager as string,",
						"          {Sales Man} as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
